{
  "compilerOptions": {
    "target": "es5",  
		"lib": ["ES2015","DOM"],      
    "module": "CommonJS",
    "allowJs": true,             
    "checkJs": true,            
    "declaration": true,  
    "outDir": "./dist",           
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "types": ["jest","node"],   
		"moduleResolution": "Node",  
		"esModuleInterop": true,   
  },
	"include": ["./src/**/*"],
	"exclude": ["node_modules","tests"]
}
